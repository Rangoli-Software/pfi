<meta charset="utf-8" />
<meta http-equiv="X-UA-Compatible" content="IE=edge" />
<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
<!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
<meta http-equiv="cache-control" content="no-cache" />
<% if ( locals.description ) { %>
<meta name="description" content="<%- description %>" />
<% } %>
<% if ( locals.keywords ) { %>
<meta name="keywords" content="<%- keywords + " , " + config.siteKeywords %>" />
<% } else { %>
<meta name="keywords" content="<%- config.siteKeywords %>" />
<% } %>
<title>
    <%- title %>
</title>

<link rel="stylesheet" href="/shopper-1.0.4/fonts/feather/feather.css">
<link href="/shopper-1.0.4/css/theme.min.css" rel="stylesheet" />
<link href="/js/pfi20082001.css" rel="stylesheet"/>
<link href="/js/icomoon.css" rel="stylesheet"/>

<script type="text/javascript" src="/js/pfiglobs20120223.js"></script>
<script type="text/javascript" src="/js/pfitmpl20122803.js"></script>
<script type="text/javascript" src="/js/pficat20122107.js"></script>
<script type="text/javascript" src="/js/pfimch20122114.js"></script>
<script type="text/javascript" src="/nc/pfifx20.js"></script>
<script type="text/javascript" src="/js/pfishop20122802.js"></script>
<script type="text/javascript" src="/js/pfiprod20122113.js"></script>
<script src="/shopper-1.0.4/libs/jquery/dist/jquery.min.js"></script>
<script src="/shopper-1.0.4/libs/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script async defer src="//assets.pinterest.com/js/pinit.js"></script>
<script async defer type="text/javascript" src="https://www.paypalobjects.com/api/checkout.js"></script>
<% if ( config.isProduction && config.isGA ) { %>
    <script async src="https://www.googletagmanager.com/gtag/js?id=<%- config.gaCode %>"></script>
<% } %>
<% if ( config.isProduction) { %>
    <% var funnel = (locals.funnel ? locals.funnel : "PageView") %>
    <% if ( config.isGA ) { %>
        <%- pfi.gaInit(config.gaCode, pfi.gaFunnel(funnel)) %>
    <% } %>
    <% if ( config.isFB ) { %>
        <%- pfi.fbInit(config.fbCode, pfi.fbFunnel(funnel)) %>
    <% } %>
<% } %>
